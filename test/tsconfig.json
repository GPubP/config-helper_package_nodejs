{
	"compilerOptions": {
		"baseUrl": ".",
		"module": "ES2022",
		"moduleResolution": "node16",
		"outDir": "dist",
		"declarationDir": "dist",
		"declaration": true,
		"target": "ES2022",
		"allowSyntheticDefaultImports": true,
		"emitDecoratorMetadata": true,
		"esModuleInterop": true,
		"experimentalDecorators": true,
		"noImplicitAny": true,
		"resolveJsonModule": true,
		"sourceMap": true,
		"strictBindCallApply": true,
		"strictFunctionTypes": true,
		"strictNullChecks": false,
		"skipLibCheck": true,
		"lib": [
			"ES2022"
		],
		"types": [
			"jest",
			"node"
		],
		"typeRoots": [
			"../node_modules/@types",
			"node_modules/@types"
		],
		"paths": {}
	},
	"include": [
		"**/*.ts"
	],
	"exclude": []
}
