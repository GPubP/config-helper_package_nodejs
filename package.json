{
  "name": "@wcm/config-helper",
  "version": "5.0.1",
  "description": "config helper",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc -w",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "test": "jest --config jest.config.js --runInBand --verbose false",
    "test:watch": "npm run test -- --watchAll",
    "install:peers": "install-peers -f",
    "docs": "npm run docs:html && npm run docs:markdown",
    "docs:html": "typedoc --plugin typedoc-plugin-missing-exports --internalModule ~internal~ --excludeExternals true --entryPoints ./src/index.ts --excludePrivate --out docs",
    "docs:markdown": "typedoc --plugin typedoc-plugin-markdown --plugin typedoc-github-wiki-theme --plugin typedoc-plugin-missing-exports --internalModule ~internal~ --excludeExternals true --entryPoints ./src/index.ts --excludePrivate --theme github-wiki --out docs/markdown"
  },
  "keywords": [
    "WCM",
    "config",
    "helper"
  ],
  "devDependencies": {
    "@nestjs/common": "10.1.0",
    "@types/cron": "^1.7.3",
    "@types/express": "4.17.7",
    "@types/jest": "29.5.2",
    "@types/jsonwebtoken": "9.0.2",
    "@types/ramda": "^0.26.43",
    "@types/sinon": "^9.0.4",
    "@types/sinon-express-mock": "1.3.9",
    "@types/uuid": "^7.0.0",
    "@typescript-eslint/eslint-plugin": "5.59.11",
    "@typescript-eslint/parser": "5.59.11",
    "eslint": "8.42.0",
    "eslint-plugin-import": "2.27.5",
    "got": "11.8.6",
    "install-peers-cli": "^2.2.0",
    "jest": "29.5.0",
    "jest-extended": "4.0.0",
    "nock": "^12.0.2",
    "rxjs": "7.8.1",
    "sinon": "^9.0.2",
    "sinon-express-mock": "^2.2.1",
    "ts-jest": "29.1.0",
    "ts-loader": "6.2.1",
    "typedoc": "0.24.8",
    "typedoc-github-wiki-theme": "1.1.0",
    "typedoc-plugin-markdown": "3.15.3",
    "typedoc-plugin-missing-exports": "2.0.0",
    "typescript": "5.1.3"
  },
  "author": "Pieterjan Van Saet <pieterjan.vansaet@studiohyperdrive.be>",
  "license": "ISC",
  "dependencies": {
    "@acpaas/kafka-nodejs-helper": "1.1.3",
    "@acpaas/monitor": "4.0.3",
    "cron": "1.8.2",
    "express": "4.18.2",
    "jsonwebtoken": "9.0.0",
    "ramda": "0.29.0",
    "uuid": "9.0.1"
  },
  "peerDependencies": {
    "@nestjs/common": "^10.1.0",
    "@wcm/axios-got-wrapper": "^2.0.0",
    "rxjs": "^7.8.1"
  }
}
